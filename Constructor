#include<stdio.h>
#include<conio.h>
#include<iostream.h>
class one
{
int len,width,input,a;
public:
one()
{
len=20;
width=25;
}
void fun()
{
cout<<"Pre Define value of Length and Width is 20 and 25 \n";
cout<<"Enter Input "<<endl;
cin>>input;
a=len*width*input;
cout<<value of length * width * input = a<<endl;
}
};
void main()
{
one abc;
abc.fun();
getch();
}
