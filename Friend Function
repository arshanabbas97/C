-------------------------------------------------------Membership Label------------------------------------------------------------
#include<stdio.h>
#include<conio.h>
#include<iostream.h>
class ONE
{
public:
int x;
void fun();
};
void ONE::fun()
{
x=10;
cout<<a;
}
void main()
{
clrscr();
ONE abc;
abc.fun();
getch();
}
-----------------------------------------------------------------------------------------------------------------------------------

------------------------------------------------------------Friend Function--------------------------------------------------------
#include<stdio.h>
#include<conio.h>
#include<iostream.h>
class ONE
{
public:
int x;
void ff()
{
x=10;
}
friend void fun();
};
void fun(ONE abc)
{
cout<<abc.x;
}
void main()
{
clrscr();
ONE abc;
abc.ff();
fun(abc);
getch();
}
-----------------------------------------------------------------------------------------------------------------------------------
